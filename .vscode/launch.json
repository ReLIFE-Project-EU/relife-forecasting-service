{
  // Simple Python script debug (run current file)
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "debugpy",
      "request": "launch",

      // Use your venv interpreter (keep this if it is correct for your project)
      "python": "/Users/dantonucci/.local/share/virtualenvs/relife-forecasting-service-2oR1Kv5U/bin/python",

      // Run the currently open script
      "program": "${file}",

      // Run in the workspace root (change if your script expects a different working dir)
      "cwd": "${workspaceFolder}",

      // Optional: pass CLI arguments to your script
      "args": [],

      // Optional: environment variables
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      },

      // Terminal / debug behavior
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}


