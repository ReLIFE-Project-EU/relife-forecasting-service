    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: FastAPI (uvicorn)",
            "type": "debugpy",
            "request": "launch",
            "python": "/Users/dantonucci/.local/share/virtualenvs/relife-forecasting-service-2oR1Kv5U/bin/python",
            "module": "uvicorn",
            "args": [
                "relife_forecasting.app:app",
                "--reload",
                "--host",
                "0.0.0.0",
                "--port",
                "9090"
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "jinja": true
        }
    ]
}
